{
  "env": {
    "test": {
      "sourceMaps": "inline",
      "retainLines": true,
      "presets": ["env"],
      "plugins": [
        "transform-object-rest-spread",
        ["transform-react-jsx", { "pragma": "h" }],
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }],
        ["istanbul", {
          "exclude": [
            "**/test/**",
            "**/dist/**"
          ]
        }]
      ]
    }
  }
}
